{
  "name": "es-expand",
  "version": "1.0.8",
  "description": "es-expand 提供了对 JS 中原生类型功能的扩展（如Object、Array、JSON、String、Number、Math、Date等等），也提供了一些工具函数；",
  "main": "dist/es-expand.cjs.js",
  "module": "dist/es-expand.esm.js",
  "browser": "dist/es-expand.umd.js",
  "types": "types/index.d.ts",
  "sideEffects": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/GuoBinyong/es-expand.git"
  },
  "keywords": [
    "扩展",
    "JS扩展",
    "JS原生扩展",
    "JavaScript扩展",
    "JavaScript原生扩展",
    "原生扩展",
    "Object",
    "Array",
    "JSON",
    "String",
    "Number",
    "Math",
    "Date"
  ],
  "author": "郭斌勇",
  "license": "ISC",
  "files": [
    "dist/[^.]*",
    "types/[^.]*"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "node test/test.js",
    "pretest": "npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "rollup": "^2.7.2",
    "rollup-plugin-babel": "^4.2.0"
  }
}
